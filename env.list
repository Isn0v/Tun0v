# REQUIRED!!! Telegram bot token 
TELEGRAM_BOT_TOKEN=<YOUR_BOT_TOKEN>

# Log level, can be DEBUG, INFO, WARNING, ERROR
LOG_LEVEL=DEBUG

# Whether to log to file (logs will be saved to logs/bot.log) or not
LOG_TO_FILE=false

# Path to the downloaded audio files
AUDIO_PATH=./downloads/audio

# Name of the audio file
AUDIO_NAME=audio

# Extension of the audio file
AUDIO_EXT=m4a

# Path to the metadata files
METADATA_PATH=./downloads/metadata

# Name of the metadata file
METADATA_NAME=media

# Extension of the metadata file
METADATA_EXT=info.json

# Timeout for bot requests in seconds
BOT_REQUEST_READ_TIMEOUT=10
